{
    "name": "dvomedia/gulp-skeleton",
    "description": "A pre-configured skeleton for using Gulp, Silex & Twig",
    "license": "GPL",
    "type": "project",
    "minimum-stability": "dev",
    "require": {
        "silex/silex": "1.0.*@dev",
        "igorw/config-service-provider": "v1.2.1",
        "twig/twig": ">=1.8,<2.0-dev",
        "guzzlehttp/guzzle": "~4.0"
    },
    "autoload": {
        "psr-0": {"DVO": "src/"}
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "1.*",
        "phpunit/phpunit": "3.7.*",
        "phpspec/phpspec": "*"
    }
}
